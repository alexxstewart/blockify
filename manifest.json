{
  "manifest_version": 2,
  "name": "Blockify",
  "version": "1.0.0",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["<all_urls>"]
  }],
  "browser_action": {
    "default_popup" :"popup.html",
    "default_title" :"Blockify"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}